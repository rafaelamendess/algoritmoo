																																																																		programa {
  funcao inicio() {
    
    //Declaraçao de variaveis 
    real v, p
    
    //Entrada
    escreva("Digite o valor que você  deseja obter 10%: ")
    leia(v)
  
    //Processamento
    p = v * 10/100

    //Saída
    escreva("O resultado é:", p)
  
  }
}
